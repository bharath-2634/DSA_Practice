Solution:
import java.util.*;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        // catalan number = 2n!/ n! * (n+1)!
        int[] dp = new int[n+1];
        dp[0] = dp[1] = 1;

        for(int i=2;i<=n;i++) {
            for(int j=1;j<=i;j++) {
                dp[i]+=dp[j-1]*dp[i-j];
            }
        }

        System.out.println(dp[n]);
    }
}
